<td>
<% if model.ppt %>
  <%= link_to(action: 'ppt', id: model.id) do %>
    <span><%= model.title %></span>
    <i class="fa-light fa-clapperboard-play has-text-grey-dark"></i>
  <% end %>
<% else %>
  <%= link_to model.title, { action: 'show', path: model.path } %>
<% end %>
</td>
<td class="has-text-right"><time data-controller="time"><%= model.updated_at.to_fs(:iso8601) %></time></td>
<td>
  <div data-show-target="item" style="visibility: hidden">

  </div>
</td>
